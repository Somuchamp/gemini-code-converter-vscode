{
 "name": "gemini-code-converter",
 "displayName": "Gemini Code Converter",
 "description": "A multi-language code converter powered by Gemini.",
 "version": "0.0.4",
 "publisher": "SoumyadeepDas",
 "icon": "icon.png",
 "engines": {
  "vscode": "^1.85.0"
 },
 "categories": [
  "Other"
 ],
 "activationEvents": [
  "onCommand:code-converter.start"
 ],
 "main": "./out/extension.js",
 "contributes": {
  "commands": [
   {
    "command": "code-converter.start",
    "title": "Start Code Converter"
   }
  ]
 },
 "scripts": {
  "vscode:prepublish": "npm run compile",
  "compile": "node esbuild.js",
  "watch": "tsc --watch -p ./",
  "pretest": "npm run compile && npm run lint",
  "lint": "eslint src --ext ts",
  "test": "node ./out/test/runTest.js"
 },
 "devDependencies": {
  "@types/mocha": "^10.0.6",
  "@types/node": "18.x",
  "@types/vscode": "^1.85.0",
  "@typescript-eslint/eslint-plugin": "^6.15.0",
  "@typescript-eslint/parser": "^6.15.0",
  "@vscode/test-electron": "^2.3.8",
  "esbuild": "^0.25.8",
  "eslint": "^8.56.0",
  "glob": "^10.3.10",
  "mocha": "^10.2.0",
  "typescript": "^5.3.3"
 },
 "dependencies": {
  "@google/generative-ai": "^0.1.3",
  "@xenova/transformers": "^2.17.2"
 }
}
